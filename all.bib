@article{kamide.y:1999,
  title = {Incremental Pre-Head Attachment in {{Japanese}} Parsing},
  author = {Kamide, Yuki and Mitchell, Don C.},
  year = {1999},
  month = oct,
  journal = {Language and Cognitive Processes},
  volume = {14},
