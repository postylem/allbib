@article{hahn.m:2022,
  title = {Morpheme Ordering across Languages Reflects Optimization for Processing Efficiency},
  author = {Hahn, Michael and Mathew, Rebecca and Degen, Judith},
  year = {2022},
  month = feb,
  journal = {Open Mind},
