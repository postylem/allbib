@article{ryskin.r:2018,
  title = {Comprehenders Model the Nature of Noise in the Environment},
  author = {Ryskin, Rachel and Futrell, Richard and Kiran, Swathi and Gibson, Edward},
  year = {2018},
  month = dec,
  journal = {Cognition},
  volume = {181},
  pages = {141--150},
